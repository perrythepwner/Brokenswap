{"ast":null,"code":"import{createReducer}from'@reduxjs/toolkit';import{addImportedFarm,initializeImportedFarm,removeImportedFarm}from'./actions';const initialState={importedFarmSummaries:[]};export default createReducer(initialState,builder=>builder.addCase(addImportedFarm,(state,_ref)=>{let{payload:{farmSummary}}=_ref;const farm=state.importedFarmSummaries.find(importedFarm=>importedFarm.stakingAddress===farmSummary.stakingAddress);const farmIndex=farm?state.importedFarmSummaries.indexOf(farm):-1;const importedFarmSummaries=[...state.importedFarmSummaries];if(farmIndex>=0){importedFarmSummaries[farmIndex]=farmSummary;}else{importedFarmSummaries.push(farmSummary);}return{importedFarmSummaries};}).addCase(removeImportedFarm,(state,_ref2)=>{let{payload:{farmAddress}}=_ref2;const farm=state.importedFarmSummaries.find(importedFarm=>importedFarm.stakingAddress===farmAddress);const farmIndex=farm?state.importedFarmSummaries.indexOf(farm):-1;const importedFarmSummaries=[...state.importedFarmSummaries];if(farmIndex>=0){importedFarmSummaries.splice(farmIndex,1);}return{importedFarmSummaries};}).addCase(initializeImportedFarm,()=>{return{importedFarmSummaries:[]};}));","map":null,"metadata":{},"sourceType":"module"}